        $(document).ready(function () {
          $('.record #btn-update').click(function () {
            var attractionId = $(this).data('id');
            // Update some info
            $('#record-id').val(attractionId);
            $('#modal-update').show();
            // ----------
            //alert('Update button clicked for record:\n' + attractionId);
          });

          $('.modal .close').click(function () {
            $('.modal').hide();
          });

        });

